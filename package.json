{
  "name": "zembil",
  "version": "1.0.0",
  "description": "Offline Package & Docs Cache for developers with unreliable internet",
  "main": "dist/index.js",
  "bin": {
    "zembil": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "offline",
    "package-manager",
    "cache",
    "documentation",
    "npm",
    "pip",
    "maven",
    "developer-tools"
  ],
  "author": "The Habeshan Nerd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AbelaTs/Zembil.git"
  },
  "homepage": "https://github.com/AbelaTs/Zembil#readme",
  "bugs": {
    "url": "https://github.com/AbelaTs/Zembil/issues"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "archiver": "^6.0.1",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^11.2.0",
    "node-fetch": "^2.7.0",
    "ora": "^5.4.1",
    "semver": "^7.5.4",
    "sqlite3": "^5.1.6",
    "tar": "^6.2.0",
    "yauzl": "^3.0.0"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/extract-zip": "^2.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.5",
    "@types/node-fetch": "^2.6.13",
    "@types/tar": "^6.0.7",
    "@types/yauzl": "^2.10.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
